#include <stdio.h> 

int main()
{
	int a, b, c, max;
	printf("请输入三个值\n");
	scanf_s("%d,%d,%d", &a, &b, &c);//若是%d%d%d则是竖的输入三个数字。
	                                //有逗号的则是加上逗号输入

	if (a > b)
	{
		max = a;
	}
	else
	{
		max = b;
	}
if (c > max)
    {
	max = c;
    }
	printf("max=%d\n", max);
	return 0;
